    "7531e1a0-6847-4a10-9c79-775c76b430c4": {
      "type": "media-with-text",
      "blocks": {
        "025468ac-0134-4455-a94d-b623cf28dd70": {
          "type": "heading",
          "settings": {
            "heading": "Installing the Brackets",
            "heading_size": "h3",
            "heading_h1": false
          }
        },
        "31d9c101-9df5-4154-ae21-035e0a4d88a2": {
          "type": "text",
          "settings": {
            "text": "<p>STEP 5 - Choose a drill bit that's smaller than your screws. Drill pilot holes through your bracket marks.<\/p><p><\/p><p>STEP 6 - Screw the brackets into place.<\/p>",
            "enlarge_text": false
          }
        }
      },
      "block_order": [
