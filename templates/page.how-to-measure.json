    "e09aaaf6-0bc2-4026-8462-7256af02f4fd": {
      "type": "multi-column",
      "blocks": {
        "template--20337082597693__e09aaaf6-0bc2-4026-8462-7256af02f4fd-column-1": {
          "type": "column",
          "settings": {
            "heading": "Window Depth",
            "text": "<p>If your window's depth is greater than 2.4\" (6.10 cm), we recommend you use inside mount blinds instead.<\/p>",
            "button_label": "",
            "button_link": ""
          }
        },
        "template--20337082597693__e09aaaf6-0bc2-4026-8462-7256af02f4fd-column-2": {
          "type": "column",
          "settings": {
            "heading": "Flat Space",
            "text": "<p>You will need at least 2\" of flat space above the window fin order to install outside mount blinds.<\/p>",
            "button_label": "",
            "button_link": ""
          }
        },
        "template--20337082597693__e09aaaf6-0bc2-4026-8462-7256af02f4fd-column-3": {
          "type": "column",
          "settings": {
            "heading": "Light Gap Coverage",
            "text": "<p>For added light gap coverage, we recommend adding 3-to-6 inches to your height measurement<\/p>",
            "button_label": "",
            "button_link": ""
          }
        }
      },
