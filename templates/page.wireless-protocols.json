    "63200481-aa41-4d7e-b345-9e00120efb51": {
      "type": "media-with-text",
      "blocks": {
        "17cb5dee-9ac5-4ba4-a801-7fdf1810baf5": {
          "type": "heading",
          "settings": {
            "heading": "Z-Wave",
            "heading_size": "h4",
            "heading_h1": false
          }
        },
        "561ead7a-3f8d-4e17-9ec5-7af97d405d5a": {
          "type": "text",
          "settings": {
            "text": "<p>Z-Wave is specifically designed for smart home devices like smart blinds. The Beasen Home Z-Wave motor is compatible with a variety ofsystems and smart devices, including Amazon Alexa, Google Assistant, SmartThings App, Home Assistant, and Hubitat.<\/p>",
            "enlarge_text": false
          }
        }
      },
      "block_order": [
        "17cb5dee-9ac5-4ba4-a801-7fdf1810baf5",
