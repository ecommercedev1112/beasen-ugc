    "fe39c609-68bc-414d-aef9-228939e7c264": {
      "type": "multi-column",
      "blocks": {
        "template--20277506310461__fe39c609-68bc-414d-aef9-228939e7c264-column-1": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/free_replacement.svg",
            "heading": "Free Replacement",
            "text": "<p>If one of your blinds or shades has been measured incorrectly, get in touch with us within 30 days of your purchase<\/p>",
            "button_label": "",
            "button_link": ""
          }
        },
        "template--20277506310461__fe39c609-68bc-414d-aef9-228939e7c264-column-2": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/1_limit.svg",
            "heading": "1 Replacement Limit",
            "text": "<p>1 remake per item; maximum of 4 item remakes per customer within 30 days from the receipt of the order (unless the error is caused by us, in which case, we will rectify it!).<\/p>",
            "button_label": "",
            "button_link": ""
          }
        },
        "template--20277506310461__fe39c609-68bc-414d-aef9-228939e7c264-column-3": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/20day.svg",
            "heading": "20-Day Fulfillment",
            "text": "<p>Once we've received your incorrect blinds, we’ll remake or adjust your order to your correct measurements for free (at your new size) within 20 days of the date your order was delivered.<\/p>",
            "button_label": "",
            "button_link": ""
          }
        },
        "deaca11a-fbd3-4e2a-bc32-1d8e0a8f772a": {
          "type": "column",
          "disabled": true,
          "settings": {
            "image": "shopify:\/\/shop_images\/banner_new_icon-22_1c6426f6-6f35-4357-bd29-ed6a83d60669.png",
            "heading": "",
            "text": "<p>✔  Once your shade or blind is with us, we’ll remake it or adjust it to your correct measurements for free (at your new size) within 20 days of the date your order was delivered.<\/p>",
            "button_label": "",
            "button_link": ""
          }
        }
      },
