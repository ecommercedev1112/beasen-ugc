          "settings": {
            "image": "shopify:\/\/shop_images\/dust_new.jpg",
            "heading": "Dust",
            "text": "<p>Keep your blinds clean and looking great by dusting every one-to-two weeks.<\/p>",
            "button_label": "",
            "button_link": ""
          }
        }
      },
